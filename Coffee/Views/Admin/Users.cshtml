@{
    ViewData["Title"] = "Users";
}

<div class="">
    <h1 class="display-4">Users</h1>
    <br />
    <br />
    <br />

    <table cellpadding="10">
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>Created</th>
            <th>Email</th>
            <th>PhoneNumber</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Created.ToString("yyyy.mm.dd hh:mm")</td>
                <td>@item.Email</td>
                <td>@item.PhoneNumber</td>
                <td><a href="/admin/news/edit/@item.Id">edit</a></td>
                <td><a href="/admin/news/delete/@item.Id">delete</a></td>
            </tr>
        }
    </table>
</div>
