@{
    ViewData["Title"] = "News";
}


<div class="">
    <h1 class="display-4">News</h1>
    <br />
    <br />

    <table cellpadding="10">
        <tr>
            <th>Title</th>
            <th>Text</th>
            <th>Date</th>
            <th>Create date</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Text</td>
                <td>@item.Date.ToString("yyyy.mm.dd hh:mm")</td>
                @*<span>@item.AuthorId</span>*@
                <td>@item.CreateDate.ToString("yyyy.mm.dd hh:mm")</td>
                @*<span>@item.IsActive</span>*@
            </tr>
        }
    </table>

</div>
